# Set a sensible default PATH
# ~/.bin is where we will keep custom executables
export PATH=$HOME/.bin:/opt/bin:/usr/local/bin:/Users/bhughes/Library/Python/3.7/bin:/usr/local/sbin:/usr/bin:/bin:$PATH

# Set a sensible default MANPATH as well
export MANPATH=/opt/man:/opt/share/man:$MANPATH

